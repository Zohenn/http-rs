matches / {
    set_header("Server", "http-rs");

    if 1 == 1 {
        set_header("Server", "http-rs2");
    }

    if 1 == 2 {
        set_header("Server", "http-rs3");
    }

    if request.method == "GET" {
        set_header("X-Method", "http-get");
    }
}

matches /index2.html {
    redirect 301 "/index.html";
}

matches /index3.html {
    return 403 "Naaaah";
}
